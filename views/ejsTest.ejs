<!DOCTYPE html>

    <h1>안녕하세요</h1>
    <h2>ejs 로 만든 페이지입니다.</h2>
    <input type="text" id="inputText"/>
    <input type="text" id="inputText2"/>
    <button id="firstButton">버튼</button>
    <script src="https://code.jquery.com/jquery-3.5.1.js" integrity="integrity//값 대입" crossorigin="anonymous"></script>
    <script>
        $("#firstButton").click(function(){
            var userInput = $("#inputText").val();
            var userInput2 = $("#inputText2").val();            
            $.ajax({
                url: "/userData",
                type: "POST",
                data: {
                    userData1 : userInput,
                    userData2 : userInput2
                },
                success: function (data) {
                    console.log(data);
                    if(data === true){
                        alert("요청 성공");
                    }
                },
            });
        })
    </script>
</html>